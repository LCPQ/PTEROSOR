      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>https://lcpq.github.io/PTEROSOR/software/</guid>
      <description>&lt;div id=&#34;software&#34;&gt;&#xA;Please wait...&#xA;&lt;/div&gt;&#xA;&#xA;&#xA;&lt;script&gt;&#xA;    window.onload = async function () {&#xA;&#xA;        /* Get list of repositories */&#xA;    //    var proxyUrl = &#39;https://cors-anywhere.herokuapp.com/&#39;;&#xA;        var proxyUrl = &#39;&#39;;&#xA;        var targetUrl = &#39;https://git.irsamc.ups-tlse.fr/api/v1/orgs/pterosor/repos&#39;;&#xA;        software = await fetch(proxyUrl + targetUrl).then(function (response) {&#xA;            return response.json();&#xA;        }).then(function (response) {&#xA;            return response;&#xA;        });&#xA;        &#xA;&#xA;        /* Display list of repositories */&#xA;        listContainer = document.getElementById(&#39;software&#39;);&#xA;        listElement = document.createElement(&#39;ul&#39;),&#xA;        listContainer.innerHTML = &#34;&#34;;&#xA;        listContainer.appendChild(listElement);&#xA;        &#xA;        for (repo in software) {&#xA;            if (software[repo].mirror) {&#xA;                name = software[repo].full_name.split(&#39;/&#39;)[1];&#xA;                description = software[repo].description;&#xA;                url = software[repo].website;&#xA;                if (url == &#34;&#34;)&#xA;                    url = software[repo].html_url;&#xA;&#xA;                listItem = document.createElement(&#39;li&#39;);&#xA;                listItem.innerHTML =&#xA;                    &#34;&lt;a href=\&#34;&#34; + url + &#34;\&#34;&gt;&#34; + name + &#34;&lt;/a&gt;&lt;br&gt;&#34;&#xA;                    + description;&#xA;                listElement.appendChild(listItem);&#xA;&#xA;            }&#xA;        }&#xA;    }&#xA;&lt;/script&gt;</description>
    </item>
